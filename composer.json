{
    "name": "teknoo/east-foundation",
    "type": "library",
    "description": "Universal package, including a symfony bundle, to allow developer to use the #East philosophy into there project with any framework supporting PSR-11 or with Symfony 3+.",
    "keywords": ["east", "controller", "design"],
    "homepage": "http://teknoo.software/east-framework",
    "license": ["MIT"],
    "authors": [
        {
            "name": "Richard DÃ©loge",
            "email": "richarddeloge@gmail.com",
            "role":"Software architect, Lead developer"
        }
    ],
    "require": {
        "php": "~7.4",
        "ext-json": "*",
        "psr/http-message": "~1.0",
        "teknoo/immutable": "^2.0.0",
        "teknoo/states": "^4.0.1",
        "teknoo/recipe": "^2.0.0",
        "psr/container-implementation": "~1.0",
        "php-di/php-di": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.5",
        "squizlabs/php_codesniffer": "~3.5",
        "pdepend/pdepend": "~2.7",
        "sebastian/phpcpd": "~4.1",
        "phploc/phploc": "~5.0",
        "phpstan/phpstan": "^0.12.8",
        "behat/behat": "~3.5",
        "symfony/framework-bundle": "~4.4||~5.0",
        "symfony/routing": "~4.4||~5.0",
        "symfony/console": "~4.4||~5.0",
        "symfony/yaml": "~4.4||~5.0",
        "symfony/templating": "~4.4||~5.0",
        "symfony/security": "~4.4||~5.0",
        "symfony/serializer": "~4.4||~5.0",
        "zendframework/zend-diactoros": "~1.8||~2.2",
        "symfony/psr-http-message-bridge":"~1.2"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Teknoo\\East\\Foundation\\": "src/universal",
            "Teknoo\\East\\FoundationBundle\\": "src/symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Teknoo\\Tests\\East\\Foundation\\": "tests/universal",
            "Teknoo\\Tests\\East\\FoundationBundle\\": "tests/symfony"
        }
    },
    "archive": {
        "exclude": ["docs", "features", ".idea"]
    },
    "extra": {
        "thanks": {
            "name": "teknoo/recipe",
            "url": "https://github.com/TeknooSoftware/recipe"
        }
    }
}
