{
    "name": "teknoo/east-framework",
    "type": "library",
    "description": "Symfony bundle to allow developer to use the #East philosophy into there Symfony project and create #east controller",
    "keywords": ["east", "#east", "controller", "design"],
    "homepage": "http://teknoo.software/east-framework",
    "license": ["MIT"],
    "authors": [
        {
            "name": "Richard DÃ©loge",
            "email": "richarddeloge@gmail.com",
            "role":"Software architect, Lead developer"
        }
    ],
    "require": {
        "php": "~7.0",
        "psr/http-message": "~1.0",
        "zendframework/zend-diactoros": "~1.0",
        "symfony/psr-http-message-bridge":"~0.2",
        "teknoo/immutable": "~0.0.1-alpha1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "symfony/symfony": "~3.0",
        "symfony/framework-bundle": "~3.0",
        "symfony/console": "~3.0",
        "symfony/validator": "~3.0",
        "symfony/monolog-bundle": "~2.9"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Teknoo\\East\\Framework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Teknoo\\Tests\\East\\Framework\\": "tests/"
        }
    },
    "archive": {
        "exclude": ["demo", "vendor", ".idea"]
    }
}
