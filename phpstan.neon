parameters:
    ignoreErrors:
        - '#[a-zA-Z0-9\\_]+::registerMiddleware\(\)#'
        -
            message: '#mixed given#'
            path: src/di.php
        -
            message: '#clone mixed#'
            path: src/di.php
        -
            message: '#[a-zA-Z0-9\\_]+::findClasses\(\)#'
            path: src/Extension/ComposerLoader.php
